<Activity mc:Ignorable="sap sap2010 sads" x:Class="ParallelActivity" mva:VisualBasic.Settings="{x:Null}" sap2010:WorkflowViewState.IdRef="ParallelActivity_1"
 xmlns="http://schemas.microsoft.com/netfx/2009/xaml/activities"
 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
 xmlns:mva="clr-namespace:Microsoft.VisualBasic.Activities;assembly=System.Activities"
 xmlns:sads="http://schemas.microsoft.com/netfx/2010/xaml/activities/debugger"
 xmlns:sap="http://schemas.microsoft.com/netfx/2009/xaml/activities/presentation"
 xmlns:sap2010="http://schemas.microsoft.com/netfx/2010/xaml/activities/presentation"
 xmlns:scg="clr-namespace:System.Collections.Generic;assembly=mscorlib"
 xmlns:sco="clr-namespace:System.Collections.ObjectModel;assembly=mscorlib"
 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Sequence DisplayName="ParallelActivity" sap2010:WorkflowViewState.IdRef="Sequence_1">
    <Parallel sap2010:WorkflowViewState.IdRef="Parallel_1">
      <Parallel.Variables>
        <Variable x:TypeArguments="x:Boolean" Name="neverSeen" />
      </Parallel.Variables>
      <Parallel.CompletionCondition>[neverSeen = False]</Parallel.CompletionCondition>
    </Parallel>
  </Sequence>
</Activity>